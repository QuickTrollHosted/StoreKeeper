<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('/bundles/storekeeperinventory/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('/bundles/storekeeperinventory/css/bootstrap-theme.min.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block javascripts %}
        <script src="{{ asset('/bundles/storekeeperinventory/js/bootstrap.min.js') }}" type="text/javascript"></script>
        {% endblock %}

        <p>
            <a href="{{ path('homepage') }}" class="btn btn-primary">{% trans %}Homepage{% endtrans %}</a>
            <a href="{{ path('part_new') }}" class="btn btn-primary">{% trans %}Add a new part{% endtrans %}</a>
            <a href="{{ path('part_stats') }}" class="btn btn-primary">{% trans %}View part statistics{% endtrans %}</a>
            <a href="{{ path('part_list') }}" class="btn btn-primary">{% trans %}View parts list{% endtrans %}</a>
            <a href="{{ path('part_view', {'id':1}) }}" class="btn btn-primary">{% trans %}View exemple part 1{% endtrans %}</a>
            <a href="{{ path('part_edit', {'id':1}) }}" class="btn btn-primary">{% trans %}Edit exemple part 1{% endtrans %}</a>
        </p>
  
        {% block body %}{% endblock %}
            
    </body>
</html>